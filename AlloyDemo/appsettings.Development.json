{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "EPiServer": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "EPiServerDB": "Data Source=(LocalDb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\AlloyDemo.mdf;Initial Catalog=AlloyDemo;Integrated Security=True;Connect Timeout=30"
  },
  "EPiServer": {
    "Cms": {
      "MappedRoles": {
        "Items": {
          "CmsEditors": {
            "MappedRoles": [ "WebEditors", "AccessToEditView" ]
          },
          "CmsAdmins": {
            "MappedRoles": [ "WebAdmins", "AccessToAdminView" ]
          },
          "EPiBetaUsers": {
            "MappedRoles": [ "Developers" ]
          },
          "VisitorGroupAdmins": {
            "MappedRoles": [ "Personalizers" ]
          }
        }
      }
    }
  }
}
